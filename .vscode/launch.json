{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "StockedUpDebug",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "KIVY_LOG_MODE" : "PYTHON",
                "PYTHON" : "./stockedup_venv/Scripts/python.exe",
                "VENV_DIR" : "./stockedup_venv",
                "GIT" : ""
            },
            "pythonArgs": ["-B"],
            "program": "./stocked_up.py",
            "args": ["--", "--data_directory", "./Data"],
            "console": "internalConsole",
            "redirectOutput": true,
            "justMyCode": false
        },
        {
            "name": "StockedUpProfile",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "KIVY_LOG_MODE" : "PYTHON",
                "PYTHON" : "./stockedup_venv/Scripts/python.exe",
                "VENV_DIR" : "./stockedup_venv",
                "GIT" : ""
            },
            "pythonArgs": ["-B"],
            "program": "./stocked_up.py",
            "args": ["--", "--data_directory", "./Data", "--profile"],
            "console": "internalConsole",
            "redirectOutput": true,
            "justMyCode": false
        },
        {
            "name": "ServerDebug",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "VENV_DIR" : "./stockedup_venv"
            },
            "pythonArgs": ["-B"],
            "program": "./run_server.py",
            "args": ["--with_tests"],
            "console": "internalConsole",
            "redirectOutput": true,
            "justMyCode": false
        }
    ]
}
  